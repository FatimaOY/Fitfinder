<Window x:Class="Fitfinder.WeekSelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select Week Number" Height="150" Width="250">
    <Grid>
        <TextBlock Text="Select Week Number:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10"/>
        <ComboBox x:Name="weekComboBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="120,10,10,0" Width="100"/>
        <Button Content="Select" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,60,0,0" Width="75" Click="SelectButton_Click"/>
    </Grid>
</Window>
