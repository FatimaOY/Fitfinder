<Window x:Class="Fitfinder.WeekSelectionClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fitfinder"
        mc:Ignorable="d"
        Title="Week Selection" Height="200" Width="400">
    <Grid>
            <TextBlock Text="Select Week Number:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10"/>
            <ComboBox x:Name="weekComboBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="120,10,10,0" Width="100"/>
            <Button Content="Select" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,60,0,0" Width="75" Click="SelectButton_Click"/>
        </Grid>
</Window>
